{
  "expo": {
    "name": "YouTube",
    "slug": "YouTube",
    "version": "1.0.0",
    "orientation": ["portrait", "landscape"],
    "icon": "./assets/icons/youtube-logo-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/yt-scrn.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/youtube-logo-icon.png",
        "backgroundColor": "#000"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icons/youtube-logo-icon.png"
    }
  }
}
